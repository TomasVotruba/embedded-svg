{
    "name": "milo/embedded-svg",
    "type": "library",
    "description": "Latte macro to embed SVG from file into HTML",
    "keywords": ["nette", "latte", "svg", "embedded", "macro"],
    "license": ["MIT"],
    "authors": [
        {
            "name": "Miloslav HÅ¯la",
            "email": "miloslav.hula@gmail.com",
            "homepage": "https://github.com/milo"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-dom": "*",
        "nette/di": "^3.0.13",
        "latte/latte": "^3.0-RC"
    },
    "autoload": {
        "psr-4": {
            "Milo\\EmbeddedSvg\\": "src"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.6",
        "symplify/easy-coding-standard": "^10.2"
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs --ansi",
        "fix-cs": "vendor/bin/ecs --ansi --fix",
        "phpstan": "vendor/bin/phpstan analyse"
    }
}
